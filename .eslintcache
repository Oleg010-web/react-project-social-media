chToProps) (Dialogs)\r\n\r\nexport default DialogsContainer;","D:\\programming\\git projects\\react-project-social-media\\react-project-social-media\\src\\context-Store.js",[],["149","150"],"D:\\programming\\git projects\\react-project-social-media\\react-project-social-media\\src\\components\\navbar\\NavbarContainer.jsx",["151","152"],"import { connect } from \"react-redux\";\r\nimport { NavLink } from \"react-router-dom\";\r\nimport Navbar from \"./Navbar\";\r\nimport navBarModules from \"./Navbar.module.css\";\r\n\r\n\r\nlet mapStateToProps = (state) => {\r\n  return {\r\n    state : state.navbarPage.navbarData\r\n  }\r\n}\r\n\r\nconst NavbarContainer = connect (mapStateToProps) (Navbar);\r\n\r\nexport default NavbarContainer;","D:\\programming\\git projects\\react-project-social-media\\react-project-social-media\\src\\components\\users\\UsersContainer.jsx",["153","154"],"import React from 'react';\r\nimport { connect } from 'react-redux';\r\nimport { follow, setCurrentPage, setIsFetching, setUsers, setTotalUsersCount, unFollow } from '../../redux/users-reducer';\r\nimport UsersStyles from './Users.module.css';\r\nimport axios from 'axios';\r\nimport Users from './Users';\r\nimport Preloader from '../common/Preloader/Preloader';\r\n\r\nclass UsersApiComponent extends React.Component {\r\n\r\n    constructor(props){\r\n        super(props);\r\n    } \r\n    componentDidMount() {\r\n        this.props.setIsFetching(true);\r\n        axios.get(`https://social-network.samuraijs.com/api/1.0/users?page=${this.props.currentValue}&count=${this.props.pageSize}`).then(Response => {\r\n            this.props.setUsers(Response.data.items);\r\n            this.props.setTotalUsersCount(Response.data.totalCount);\r\n            this.props.setIsFetching(false);\r\n        });\r\n    }\r\n    onPageChanged = (pageNumber) => {\r\n        this.props.setCurrentPage(pageNumber)\r\n        this.props.setIsFetching(true);\r\n        axios.get(`https://social-network.samuraijs.com/api/1